<div class="mytitle">
    <h2>Sản phẩm {{namelist}}</h2>
    <hr>
</div>
<div class="mygroupcard">
    {{#each this.list}}
    <div class="card zoom img-card mt-2 ml-5 mr-1" onclick="detailItem()">
        <img src="Images/{{Img}}" class="card-img-top" alt="..." height="130">
        <div class="card-body mt-3">
            <h5 class="card-title mb-4">{{ProName}}</h5>
            <p class="card-text text-center"><i class="fa fa-gavel" aria-hidden="true"></i> {{Price}}</p>
            <p class="card-text timer text-center"></p>
            <a class="btn btn-primary" href="/user/edit/{{ProID}}" role="button">
            <i class="fa fa-pencil" aria-hidden="true"></i> Edit
            </a>
        </div>
    </div>
    {{/each}}
</div>
<nav aria-label="Page navigation example">
    <ul class="pagination justify-content-center mt-4">
        <li class="page-item disabled">
            <a class="page-link" href="#" tabindex="-1" aria-disabled="true">Previous</a>
        </li>
        <li class="page-item active"><a class="page-link" href="#">1</a></li>
        <li class="page-item disabled"><a class="page-link" href="#" tabindex="-1" aria-disabled="true">2</a></li>
        <li class="page-item disabled"><a class="page-link" href="#" tabindex="-1" aria-disabled="true">3</a></li>
        <li class="page-item disabled">
            <a class="page-link" href="#" tabindex="-1" aria-disabled="true">Next</a>
        </li>
    </ul>
</nav>