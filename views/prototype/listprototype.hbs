<div class="mytitle">
    <h2>Sản phẩm {{namelist}}</h2>
    <hr>
</div>
<div class="mygroupcard">
    {{#each this.list}}
    <div class="card zoom" id="mycard">
        <img src="Images/{{image}}" class="card-img-top" alt="...">
        <div class="card-body">
            <h5 class="card-title">{{namePrototype}}</h5>
            <p class="card-text"></p>
            <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
            <a name="" id="" class="btn btn-primary" href={{linkdetail}} rel="stylesheet" role="button">Xem thêm --></a>
        </div>
    </div>
    {{/each}}
</div>